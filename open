import subprocess
import Foundation

func checkGitHubRepo() {
    let url = URL(string: "https://api.github.com/repos/MVPuknowme/Aura-core/contents")!
    var request = URLRequest(url: url)
    request.setValue("token ghp_yourGeneratedTokenHere", forHTTPHeaderField: "Authorization")
    
    URLSession.shared.dataTask(with: request) { data, response, error in
        if let data = data {
            if let json = try? JSONSerialization.jsonObject(with: data) {
                print("✅ GitHub API Response:", json)
            } else {
                print("❌ JSON Parse Failed")
            }
        } else {
            print("❌ GitHub Call Failed:", error?.localizedDescription ?? "Unknown error")
        }
    }.resume()
}

